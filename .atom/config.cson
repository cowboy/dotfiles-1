'editor':
  'fontSize': 13
  'fontFamily': 'Fantasque Sans Mono'
  'showIndentGuide': true
  'showInvisibles': true
  'softTabs': false
  'softWrap': true
  'tabLength': 4
  'invisibles':
    'cr': '↩'
    'eol': ''
    'space': '·'
    'tab': '⇥'
'core':
  'disabledPackages': [
    'metrics'
    'exception-reporting'
    'welcome'
  ]
