#CompressionLevel 6
ControlMaster auto
ControlPath ~/.ssh/master-%r@%h:%p
#ForwardAgent yes
#ForwardX11 yes
#ForwardX11Trusted yes
HashKnownHosts no
ServerAliveInterval 60
UserKnownHostsFile /dev/null ~/.ssh/known_hosts

Host ariadne belana callisto janus rhea
